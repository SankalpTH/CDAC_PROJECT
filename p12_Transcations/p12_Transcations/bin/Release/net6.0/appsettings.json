{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "p12Transactions" // mention your app name . Dont use _ in name.
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8121/eureka/", //Eureka service
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "port": 9124 // mention your port no of http url
    }
  },


  "AllowedHosts": "*"
}
